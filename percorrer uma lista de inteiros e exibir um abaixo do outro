#include <iostream>

int main() {
    std::cout << "Digite cinco números inteiros:\n";
    int numeros[5];

    std::cin >> numeros[0] >> numeros[1] >> numeros[2] >> numeros[3] >> numeros[4];
    std::cout << "Os números apresentados foram:\n";

    for(int i = 0; i < 5; i++) {
        std::cout << numeros[i] << std::endl;
    }

    return 0;
}
